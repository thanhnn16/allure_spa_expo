{
  "expo": {
    "name": "Allure Spa",
    "slug": "allure_spa_expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "allurespa",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.faithnippon.allurespa",
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "package": "com.faithnippon.allurespa",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/AlexBrush-Regular.ttf",
            "./assets/fonts/SFProText-Bold.otf",
            "./assets/fonts/SFProText-Semibold.otf",
            "./assets/fonts/SFProText-Medium.otf",
            "./assets/fonts/SFProText-Regular.otf",
            "./assets/fonts/KaiseiTokumin-Regular.ttf"
          ]
        }
      ],
      "expo-localization",
      "expo-location",
      "expo-av"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "33c05c90-3876-4bf5-b86d-9b112d505205"
      }
    }
  }
}
